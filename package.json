{
  "name": "medieval-commanders-collection",
  "version": "1.0.0",
  "description": "A card collection website for medieval commanders",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "node server/index.js",
    "postinstall": "cd server && npm install",
    "db:generate": "npx prisma generate --schema=./prisma/schema.railway.prisma",
    "db:generate:dev": "npx prisma generate --schema=./prisma/schema.dev.prisma",
    "db:push": "npx prisma db push --schema=./prisma/schema.railway.prisma",
    "db:push:dev": "npx prisma db push --schema=./prisma/schema.dev.prisma",
    "db:deploy": "npx prisma generate --schema=./prisma/schema.railway.prisma && npx prisma db push --schema=./prisma/schema.railway.prisma",
    "db:setup:dev": "npx prisma generate --schema=./prisma/schema.dev.prisma && npx prisma db push --schema=./prisma/schema.dev.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.263.1",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0"
  }
}
