# Database (use Postgres in all environments)
# Example dev: postgresql://user:pass@localhost:5432/medcom_dev?schema=public
# Example prod (Railway): postgresql://username:password@host:port/database
DATABASE_URL="postgresql://user:pass@localhost:5432/medcom_dev?schema=public"

# Server
PORT=5001
NODE_ENV=development

# Frontend
VITE_API_URL=http://localhost:5001

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change_me
JWT_SECRET=dev_secret_change_me
# Examples: 15m, 1h, 7d
JWT_EXPIRES_IN=1h
